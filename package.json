{
	"name": "eslint-config-airbnb-ts",
	"type": "module",
	"version": "0.0.1",
	"description": "Unofficial migration of the airbnb styleguide from eslintrc format to flat config file format",
	"author": "error-four-o-four",
	"license": "MIT",
	"homepage": "https://github.com/error-four-o-four/eslint-config-airbnb-ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/error-four-o-four/eslint-config-airbnb-ts.git"
	},
	"bugs": {
		"url": "https://github.com/error-four-o-four/eslint-config-airbnb-ts/issues"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"eslint-flat-config",
		"airbnb",
		"styleguide",
		"style-guide",
		"unofficial"
	],
	"files": [
		"dist",
		"README.md"
	],
	"types": "./dist/configs/index.d.ts",
	"module": "./dist/configs/index.js",
	"exports": {
		".": {
			"types": "./dist/configs/index.d.ts",
			"import": "./dist/configs/index.js"
		},
		"./base": {
			"types": "./dist/base/index.d.ts",
			"import": "./dist/base/index.js"
		},
		"./compat": {
			"types": "./dist/compat/index.d.ts",
			"import": "./dist/compat/index.js"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint:check": "eslint .",
		"lint:fix": "eslint . --fix",
		"type:check": "tsc -p tsconfig.json",
		"node:compat": "tsx scripts/compatConfigs.ts",
		"node:extract": "tsx scripts/extractData.ts",
		"node:generate": "tsx scripts/generateConfigs.ts",
		"node:merge": "tsx scripts/mergeConfigs.ts",
		"clean:base": "rimraf dist/base",
		"build:base": "tsup-node --entry.index src/defineBaseConfig.ts --format esm --dts -d ./dist/base",
		"clean:compat": "rimraf dist/compat",
		"build:compat": "tsup-node --entry.index src/configs/airbnb/index.ts --format esm --minify --dts -d ./dist/compat",
		"clean:configs": "rimraf dist/configs",
		"build:configs": "tsup-node --entry.index src/configs/custom/index.ts --format esm --minify --dts -d ./dist/configs",
		"clean": "rimraf dist/",
		"build": "npm run clean && npm run build:all",
		"build:all": "npm run build:base && npm run build:compat && npm run build:configs"
	},
	"dependencies": {
		"@stylistic/eslint-plugin": "^2.1.0",
		"eslint": "^9.4.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import-x": "^0.5.1",
		"eslint-plugin-n": "^17.7.0",
		"globals": "^15.3.0",
		"local-pkg": "^0.5.0",
		"typescript-eslint": "^8.0.0-alpha.24"
	},
	"devDependencies": {
		"@eslint/compat": "^1.0.3",
		"@eslint/eslintrc": "^3.1.0",
		"@tsconfig/node18": "^18.2.4",
		"@types/eslint": "^8.56.10",
		"@types/eslint__eslintrc": "^2.1.1",
		"@types/node": "^20.13.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.29.1",
		"rimraf": "^5.0.7",
		"ts-deepmerge": "^7.0.0",
		"tslib": "^2.6.2",
		"tsup": "^8.0.2",
		"tsx": "^4.11.2",
		"type-fest": "^4.18.3",
		"typescript": "^5.4.5"
	}
}
